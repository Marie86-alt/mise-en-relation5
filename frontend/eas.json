        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
       "EXPO_PUBLIC_BACKEND_URL": "https://mise-en-relation5-production-7e5b.up.railway.app"

      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_BACKEND_URL": "https://mise-en-relation5-production.up.railway.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
